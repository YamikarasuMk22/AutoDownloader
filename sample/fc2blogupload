	var FileManager = function(option){
		this.baseUrl    = option.url;
		this.processing = false;
		this.isSsl      = (location.protocol === "https:");
	};


	FileManager.prototype.setProcessing = function(bool){
		this.processing = bool;
	};


	FileManager.prototype.isProcessing = function(){
		return this.processing;
	};


	FileManager.prototype.send = function(method, query, data, dataType){
		var method  = method || "GET";
		var query   = query || "";
		var data    = data || {};
		var dataType= dataType || "";
		var ajax = {
			type: method,
			url : this.baseUrl + query,
			data: data
		};
		if (dataType) {
			ajax.dataType = dataType;
		};
		return $.ajax(ajax);
	};


	FileManager.prototype.trimImage = function() {
		var imgs = $(".sh_manage_table").find(".thumb").find("img");
		var i = 0;
		var t = setInterval(function(){
			if (i > imgs.length - 1) {
				clearInterval(t);
				return;
			};
			var elm = $(imgs[i]);
			var size = elm.parent().width();
			var src = elm.attr("src");
			var image = new Image();
			image.onload = function(){
				_trimImage(elm, size, image.width, image.height);
				elm.show().css({
					opacity: 0
				}).animate({
					opacity: 1.0
				}, 100);
			};
			image.src = src;
			i++;
		}, 30);

		function _trimImage(img, size, width, height){
			if (width > size || height > size) {
				var ratio = 1;
				if (width > height) {
					ratio = Math.ceil(width / height * 100) / 100;
					var w = size * ratio;
					img.css({
						width   : w + "px",
						height  : size + "px",
						position: "absolute",
						top     : 0,
						left    : ((-w + size) / 2) + "px"
					});
				} else {
					ratio = Math.ceil(height / width * 100) / 100;
					var h = size * ratio;
					img.css({
						width   : size + "px",
						height  : h + "px",
						position: "absolute",
						top     : ((-h + size) / 2) + "px",
						left    : 0
					});
				}
			} else {
				img.css({
					position: "absolute",
					top     : ((size - height) / 2) + "px",
					left    : ((size - width) / 2) + "px"
				});
			};
		};
	};


	FileManager.prototype.event = {}
	FileManager.prototype.event.unload = function(e) {
		var confirmationMessage = "この画面から移動してよろしいですか？\nファイルのアップロードは中断されます。";
		e.returnValue = confirmationMessage;     // Gecko and Trident
		return confirmationMessage;              // Gecko and WebKit
	};


	FileManager.prototype.event.onclickPaging = function(obj) {
		if (!obj) return;
		var self = this;
		var curList = $("#file_list");

		loadingImage.dist = $(document.file).children()[0];
		loadingImage.show();
		curList.css("visibility", "hidden");
		$("div.page").css("visibility", "hidden");

		var form = document.file;
		var tail = (search.tail)? search.tail: "";
		var sort = (search.sort)? search.sort: "";
		var key  = (search.key)? search.key: "";

		var param = {
			ajax   : "getFiles",
			tail   : tail,
			sort   : sort,
			key    : key,
			mode   : "control",
			process: "upload",
			type   : "",
			crc    : $(form.crc).val(),
			insert : imgInsert,
			page   : obj.data.page
		};

		fileManager.send("POST", "", param, "html").done(function(data) {
			loadingImage.hide();
			if(!data) return;
			// var list = data.listFiles;
			$("div.page").remove();
			var timer = setTimeout(function(){
				$(document.file).append(data);
				addStripeFileList();
				curList.remove();

				generatePagingButton();

				fileManager.trimImage();
				clearTimeout(timer);
			}, 100);
		}).always(function(XMLHttpRequest, textStatus) {
			loadingImage.hide();
			$(self).prop("disabled", false);
		});
	};


	FileManager.prototype.event.onBlurTitle = function() {
		$(this).off("forcus");
		$(this).val("").addClass("textboxTitle").removeClass("textboxTitle_active");
	};


	/* textbox の初期値「タイトル」を消す */
	FileManager.prototype.event.onFocusTitle = function(){
		$(this).val("").addClass("textboxTitle_active").removeClass("textboxTitle");
		var id = $(this).parent().parent().parent().parent().attr("cueue_id");
		var c  = wu.getCueue(id);
		c.edited = true;
	};


	FileManager.prototype.event.oncancelCueue = function() {
		if (dialog) return;
		var elm = $(this).parent();
		var id = elm.attr("cueue_id");
		wu.deleteCueue(id);
		elm.remove();
		toggleAlbumCheckBox();
		if (!wu.getCueueList().length) {
			setNotifyText(true);
			toggleHeaderRow(false);
		}
	};


	FileManager.prototype.event.onclickUploaderToggle = function() {
		$(this).hide();
		if ($("#multiFileUploader:visible").length) {
			showHTMLUploadForm();
		} else {
			showAjaxUploadForm();
		}
		setOption();
		$(".style_error").empty();
	};


	FileManager.prototype.event.onclickUploadButton = function() {
		var self = this;
		var cueueNum = wu.getCueueList().length;
		if (0 == cueueNum) return;

		if (!$("input.confirmFlgChkBox:checked").length) {
			var msg = "";
			if ("force" == $("#multiFileUploader .selectOverwrite").linkselect("val")){
				/* 上書き */
				msg = wu.Message.CONFIRM.OVER_WRITE_FILE;
			} else {
				/* 別名 */
				msg = wu.Message.CONFIRM.SAVE_AS_FILE;
			};
			cc.setMsg(msg);
			cc.setCallback(confirmCallback, self);
			cc.show();
		} else {
			startUpload();
		};
	};

	FileManager.prototype.event.toggleAlbumAllCheckBox = function() {
		if ($("input.albumAllCheck:checked").length) {
			$("#multiFileUploader").find("input[name=album]").prop({"checked": true});
		} else {
			$("#multiFileUploader").find("input[name=album]").prop({"checked": false});
		};
	};


	FileManager.prototype.event.togglePermissionElm = function() {
		var sel = $(this).val();
		var elm = $("#addAlbumContainer").find(".albumPasswordRow");
		if (2 == sel) {
			elm.show();
		} else {
			elm.hide();
		};
	};


	var wu, cc, ui, album, cp;
	var draggable = true;
	var imgSrc    = "//static.fc2.com/image/ja/admin/upload/28x28loading.gif";
