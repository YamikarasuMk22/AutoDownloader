	var chkconfig   = "0";
	var thumbconfig = "0";
	var exifconfig  = "1";

	var operatingMode = (function(){
		var num = 0;
		if (5000000 < maxsize) num = 3;
		else if (5000000 < maxsize) num = 2;
		else if (2000000 < maxsize) num = 1;
		else if (2000000 < maxsize) num = 0;
		return num;
	})();

	var search = {
		tail: "",
		sort: "dd",
		key: ""
	};

