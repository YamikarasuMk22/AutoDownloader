			<script type="text/javascript">
		$.fn.extend({
				"stripe": function(){
				var cnt=0;
				this.each(function(i,n){
					if(++cnt % 2 == 0){
						$(n).addClass("evencell");
					}
				});
			}
		});
		$(function(){
