	var totalLimit= ("10240") - 0;
	var curUsage  = ("42.33") - 0;
	var url = "";
	if (server == 'blog') {
		url = controlUrl + "control_upload.php?mode=control&process=upload&";
	} else {
		url = "//" + server + ".fc2.com/control_upload.php?mode=control&process=upload&";
	};
	var fileManager = new FileManager({url: url});

