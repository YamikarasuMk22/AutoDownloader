	/* check ie version */
	var ie = (function(){
		var undef, v = 3, div = document.createElement('div');
		while (
			div.innerHTML = '<!--[if gt IE '+(++v)+']><i></i><![endif]-->',
			div.getElementsByTagName('i')[0]
		);
		return v > 4 ? v : undef;
	}());
	var isFlashInstalled = function(){if(navigator.plugins["Shockwave Flash"]){return true;}try{new ActiveXObject("ShockwaveFlash.ShockwaveFlash");return true;}catch(a){return false;}}();
	var uaStr = document.upload.ua.value;

	/* file list の読み込み */
	var refleshFileList = function() {
		imgInsert = (imgInsert)? imgInsert: "";

		var param = {
			insert: imgInsert
		};

		loadingImage.show();
		// $(".style_error").empty();
		$("#file_list").css("visibility", "hidden").empty();
		fileManager.send("get", "ajax=getFiles", param, "html").done(function(data) {
			loadingImage.hide();
			if(!data) return;
			$("#file_list").remove();
			$("div.page").remove();
			var ajaxTimer = setTimeout(function(){
				$(document.file).append(data);
				addStripeFileList();
				generatePagingButton();
				fileManager.trimImage();
				clearTimeout(ajaxTimer);
			}, 100);
		}).fail(function() {
			console.log(arguments);
		});
		page = "";
	};

	/* file list の検索 */
	var searchFileList = function(page) {
		page = (parseInt(page))? parseInt(page) : "";
		var self = $("#searchButton");
		var curList = $("#file_list");
		$(self).prop("disabled", true);
		loadingImage.dist = $(document.file).children()[0];
		loadingImage.show();
		curList.css("visibility", "hidden");
		$("div.page").css("visibility", "hidden");

		var form = document.file;
		search.tail = $(form.tail).val();
		search.sort = $(form.sort).val();
		search.key  = $(form.key).val();

		var param = {
			ajax   : "getFiles",
			tail   : search.tail,
			sort   : search.sort,
			key    : search.key,
			mode   : "control",
			process: "upload",
			type   : "",
			crc    : $(form.crc).val(),
			insert : imgInsert,
			page   : page
		};

		fileManager.send("POST", "", param, "html").done(function(data) {
			if(!data) return;
			$("div.page").remove();
			var timer = setTimeout(function(){
				$(document.file).append(data);
				addStripeFileList();
				curList.remove();

				generatePagingButton();
				$(self).prop("disabled", false);
				fileManager.trimImage();

				clearTimeout(timer);
			}, 100);
		}).fail(function(XMLHttpRequest, textStatus, errorThrown) {
			console.log([XMLHttpRequest, textStatus, errorThrown]);
		}).always(function(XMLHttpRequest, textStatus) {
			loadingImage.hide();
		});
	};

	var generatePagingButton = function() {
		var curList = $("#file_list");
		var pageElms = $("div.page a");
		for (var i = 0; i < pageElms.length; i++) {
			var url = $(pageElms[i]).attr("href");
			if (!url.match(/page=[0-9]+/)) continue;

			var page = url.match(/page=[0-9]+/)[0].replace("page=", "");
			$(pageElms[i]).on("mouseup", {page: page}, fileManager.event.onclickPaging);
			$(pageElms[i]).attr("href", "javascript: void(0)");
		};
	};

	/* form の中にloading image を表示、非表示 */
	var loadingImage = {
		dist: "",
		show: function() {
			if (img) $(img).remove();
			img = document.createElement("img");
			img.setAttribute("src", imgSrc);
			this.dist = (this.dist)? this.dist: document.file;
			$(this.dist).after(img);
		},
		hide: function() {
			$(img).remove();
		}
	};

	/* file list にストライプを適用 */
	var addStripeFileList = function() {
		$("#file_list tr:even").addClass("even");
		$("#file_list tr:odd").addClass("odd");
	};

	/* プレースフォルダに SWF object が作られたどうかを返す */
	var isValidateSWF = function() {
		return ($("#spanButtonPlaceHolder").length)? false: true;
	};

	/* flashが入っていないか無効 */
	var noSWF = function() {
		$("#multiFileUploader").children().hide();
		$("#multiFileUploader").find(".noSWFContent").show();
	};

	var onDrap = function(e) {
		e.preventDefault();
		if (!draggable) return;
		$(".dragPanel").hide();
		$(".style_error").empty();
		var files = e.originalEvent.dataTransfer.files;
		console.log(files);
		addUploadCueue(files);
	};

	var addUploadCueue = function (files) {
		var msg = [];
		var obj = null;
		if (wu.getValues("maxCueue") < files.length + wu.getCueueList().length) {
			errorMessage(wu.Message.NOTICE.QUEUE_LIMIT_EXCEEDED);
			return;
		};
		for (var i = 0; i < files.length; i++) {
			var res = wu.validateFile(files[i]);
			if (res === 9001 && "function" == typeof showSuggestDialog) {
				// dialog
				showSuggestDialog();
				return;
			} else if (res !== true) {
				errorMessage(res);
				return;
			};
			var formdata = new FormData();
			var apiObj = {
				form  : formdata,
				name  : files[i].name,
				type  : files[i].type,
				size  : files[i].size,
				edited: false
			};
			formdata.append("upfile[]", files[i]);
			obj = wu.addCueue(apiObj);
			if (500 == obj) {
				/* error code 500: cueue limit 処理はここで中止 */
				errorMessage(wu.Message.NOTICE.QUEUE_LIMIT_EXCEEDED);
				return;
			};
			showCueueInfo(obj, files[i]);
		};
		toggleAlbumCheckBox();
	};

	var errorMsgCount = 0;
	var msg = [];
	var errorMessage = function(str) {
		/* swfからは一つのファイルごとにエラー処理が呼ばれるようなのでここで溜める */
		errorMsgCount++;
		msg.push(str);

		var timer = setInterval(function(){
			if (0 < errorMsgCount) {
				errorMsgCount--;
				return;
			};
			if (1 > msg.length) return;
			var txt = msg[0];
			msg = [];
			errorMsgCount = 0;
			clearInterval(timer);
			alert(txt);
		}, 200);
	};

	/* アルバム機能 */
	var openAddAlbumContainer = function() {
		if ("addAlbum" != $("#multiFileUploader").find(".selectAlbumUK").linkselect("val")) return;
		if (albumLimit <= $("#selectAlbumUK_list").find("li").length - 1) {
			var msg = wu.Message.NOTICE.ALBUM_LIMIT_EXCEEDED;
			alert(msg); return false;
		};
		draggable = false;
		dialog    = true;
		resetAddAlbumContainer();
		album.show();
		return false;
	};

	var closeAddAlbumContainer = function() {
		draggable = true;
		dialog    = false;
		album.hide();
	};

	var noAlbumItem = function(e) {
		if (1 < $("#selectAlbumUK_list").find("li").length) {
			$(this).off("mousedown", e);
			return;
		};
		openAddAlbumContainer();
	};

	var resetAddAlbumContainer = function() {
		document.addAlbumForm.reset();
		$("#addAlbumContainer").find(".errorMessage").text("").hide();
		$("#addAlbumContainer").find(".albumPasswordRow").hide();
	};

	var doAddAlbum = function() {
		$("#addAlbumContainer").find(".errorMessage").text("");
		album.send(function(data) {

			if (!data) { alert("error"); };

			if ("object" == typeof data.result) {
				var res  = data.result;
				if (1 > res.length) {
					return;
					alert("error");
				};
				for (var i = 0; i < res.length; i++) {
					var code = res[i].code;
					var msg  = res[i].msg;
					if (1 == code) {
						closeAddAlbumContainer();
						alert(msg);

						if (!isExistAlbum) {
							isExistAlbum = true;
							$("#multiFileUploader").find(".albumAllCheck").prop("disabled", false);
							$("#multiFileUploader").find("input[name=album]").prop("disabled", false);
						}

						var albumList = data.album;
						$("#selectAlbumUK").linkselect("destroy");
						var time = setTimeout(function() {
							/* select 作り直し */
							$("#selectAlbumUK").empty();
							var elm = document.createElement("option");
							elm.setAttribute("value", "addAlbum");
							elm.appendChild(document.createTextNode(wu.Message.TITLE.NEWFLASH_ALBUMTITLE));
							$("#selectAlbumUK").append(elm);
							for (var i = 0; i < albumList.length; i++) {
								var option = document.createElement("option");
								option.setAttribute("value", albumList[i].uk);
								option.appendChild(document.createTextNode(albumList[i].name));
								$("#selectAlbumUK").append(option);
							};
							$("select#selectAlbumUK").linkselect({change: openAddAlbumContainer});
							var time2 = setTimeout(function(){
								console.log(albumList[0].uk);
								$("#selectAlbumUK").linkselect("val", albumList[0].uk);
								clearTimeout(time2);
							}, 50);
							clearTimeout(time);
						}, 50);
						break;

					} else if (0 == code) {

					} else if (3001 == code) {
						/* error_album_limit_over */
						closeAddAlbumContainer();
						alert(msg);
						break;
					};

					if (1001 == code || 1002 == code) {
						/* error_album_name */
						$("#addAlbumContainer").find(".add_album_title").find(".errorMessage").text(msg).css("display", "inline-block");
					};
					if (2001 == code) {
						/* error_bad_passwd */
						$("#addAlbumContainer").find(".albumPasswordRow").find(".errorMessage").text(msg).css("display", "inline-block");
					};
				};
			};
		});
	};

	/* Event : open swf dialog */
	var dialogStart = function(flg) {
		if (this._mask) $(this._mask).remove();
		if (true === flg) {
			draggable = false;
			dialog    = true;
			this._mask = document.createElement("div");
			var w = $("#multiFileUploader").width() + "px";
			var h = $("#multiFileUploader").height() + "px";
			$(this._mask)
				.width(w)
				.height(h)
				.css({
					"background": "#fff",
					"position"  : "absolute",
					"top"       : "0",
					"left"      : "0",
					"z-index"   : "9999",
					"opacity"   : "0.1"
				});
			$("#multiFileUploader").append(this._mask);
		} else {
			draggable = true;
			dialog    = false;
			$(".style_error").empty();
		};
	};

	var showCueueInfo = function(cueueObj, file) {
		if (!cueueObj) return;
		var file = file || null;

		var cueueId  = cueueObj._cueue_id;
		var fileName = cueueObj.name;
		var html     = generateCueueHtml(cueueId, fileName);
		var fileType = fileName.match(wu.getValues("fileType"));
		var thumbnail = $(html).find(".uploadQueueThumbnail");
		if (fileType && file) {
			fileType = fileType[0].toLowerCase();
			if (fileType.match(/jpe?g|png|gif/)) {
				thumbnail.addClass("icon_image");
				getImageDataURL(file).then(function(data){
					if (!data) return false;
					thumbnail.css("background-image", "url(" + data + ")");
				}, function(){

				});
			} else {
				thumbnail.addClass("icon_" + fileType);
			};
		} else {
			thumbnail.addClass("unknow_file");
		};

		$("div.fileListView > ul").append(html);
		var timer = setTimeout(function() {
			if (!not_add_album && isExistAlbum) {
				$("li[cueue_id=" + cueueId + "]").find("input[name=album]").prop("checked", true);
			};
			if (!isExistAlbum) {
				$("#multiFileUploader").find(".albumAllCheck").prop("disabled", true);
				$("li[cueue_id=" + cueueId + "]").find("input[name=album]").prop("disabled", true);
			};
			html.css("visibility", "");
			setNotifyText(false);
			toggleHeaderRow(true);
			toggleAlbumCheckBox();

			$("div.fileListView > ul > li:even").addClass("even");
			$("div.fileListView > ul > li:odd").addClass("odd");
			clearTimeout(timer);
		}, 0);
	};

	var initCueueHtml = function() {
		var fileview = $(".fileListView > ul > li");
		if (fileview) {
			cloneCueue = fileview.clone();
			fileview.remove();
		}
	};

	var generateCueueHtml = function(id, name) {
		if (!cloneCueue) return;
		var elm = cloneCueue.clone();
		elm.attr("cueue_id", id)
			.find(".mainview").find(".filename")
			.text(name);
		return elm;
	};

	var getImageDataURL = function(file) {
		if ("undefined" == typeof FileReader) return;
		var reader = new FileReader();
		var deferred = $.Deferred();
		reader.onload = function() {
			deferred.resolve(reader.result);
		};
		reader.readAsDataURL(file);
		return deferred.promise();
	};

	var confirmCallback = function(b, elm) {
		cc.hide();
		if (true === b) {
			$(elm).prop("disabled", true);
			wu.setDisabled(true);
			startUpload();
		};
		return;
	};

	var startUpload = function() {
		$("li[cueue_id]").find("span.cueueCancelButton").remove();
		$("#multiFileUploader").find(".albumAllCheck").prop("disabled", true);
		$("li[cueue_id]").find("input").prop("disabled", true);
		var cueueNum = wu.getCueueList().length;
		var count = 0;
		var uploadtimer = setInterval(function(){
			if (count >= cueueNum) {
				clearInterval(uploadtimer);
				return;
			};
			var cueue_id = $(".fileListView > ul > li:eq(" + count + ")").attr("cueue_id");
			console.log({cueueId: cueue_id});
			draggable = false;
			wu.doUpload(cueue_id);
			count++;
		}, 1500);

		/* upload監視 */
		var resulttimer = setInterval(function() {
			var flg = wu.isComplete();
			if (!flg) return;
			console.log("finished: onCompleteUpload");
			// clearTimeout(timeout);
			clearInterval(resulttimer);
			onCompleteUpload();
		}, 1000);

		wu.doUploadSWF();
		wu.showProgressBar();
		fileManager.setProcessing(true);
		$(window).on("beforeunload", fileManager.event.unload);
	};

	var doDestroy = function() {
		/* 必須 counter IE Memory leak */
		wu.clear();
	};
	var onCompleteUpload = function() {
		wu.clearCueue();
		refleshFileList();
		$("#multiFileUploader").find(".startUploadButton").prop("disabled", false);
		wu.setDisabled(false);
		draggable = true;
		$(window).off("beforeunload", fileManager.event.unload);
	};

	var toggleThumbSize = function() {
		if($("#multiFileUploader").find("input[name=thumb]:checked").length) {
			$("#multiFileUploader").find(".thumb_size").prop("disabled", false);
		} else {
			$("#multiFileUploader").find(".thumb_size").prop("disabled", true);
		};
	};

	var toggleAlbumCheckBox = function() {
		if (!isExistAlbum) return;

		var input = $("#multiFileUploader").find("input[name=album]");
		for (var i = 0; i < input.length; i++) {
			if (!input[i].checked) {
				$("input.albumAllCheck").prop({"checked": false});
				return;
			};
		};
		$("input.albumAllCheck").prop({"checked": true});
	};

	var showAjaxUploadForm = function() {
		$("#multiFileUploader:hidden").show();
		$("form[name=upload]:visible").hide();
		$(".uploaderToggle").find("span[mode=toOld]").show();
		$(".uploaderToggle").find("span[mode=toNew]").hide();
	};
	var showHTMLUploadForm = function() {
		$("#multiFileUploader:visible").hide();
		$("form[name=upload]:hidden").show();
		$(".uploaderToggle").find("span[mode=toNew]").show();
		$(".uploaderToggle").find("span[mode=toOld]").hide();
	};
	var setOption = function() {
		var timer = setTimeout(function(){
			var mode  = $(".uploaderToggle span:visible").attr("mode");
			var chk   = ($(".confirmFlgChkBox:checked").length)? 0: 1;
			var thumb = $("input[type=checkbox][name=thumb]:visible").is(':checked');
			var exif  = $("input[name=exif]:visible").is(":checked");
			var param = {
				mode   : "control",
				process: "upload",
				type   : "",
				crc    : $(document.file.crc).val()
			};
			param.ui    = ("toOld" == mode)? 1: 0;
			param.chk   = (!chk)? 0: 1;
			param.thumb = (!thumb)? 0: 1;
			param.exif  = (!exif)? 0: 1;

			fileManager.send("POST", "ajax=setOption", param).done(function(data){
				if (!data) return;
			}).fail(function(XMLHttpRequest, textStatus, errorThrown){
				console.log([XMLHttpRequest, textStatus, errorThrown]);
			});
			clearTimeout(timer);
		}, 300);
	};

	var toggleConfirmFlg = function(e) {
		if ($("#customConfirmBox").find("input.confirmFlgChkBox:checked").length) {
			$("#multiFileUploader").find("input.confirmFlgChkBox").prop({"checked": true});
			setOption();
		} else {
			$("#multiFileUploader").find("input.confirmFlgChkBox:checked").prop({"checked": false});
			setOption();
		};
	};
	var toggleConfirmFlg_s = function(e) {
		if ($("#multiFileUploader").find("input.confirmFlgChkBox:checked").length) {
			$("#customConfirmBox").find("input.confirmFlgChkBox").prop({"checked": true});
			setOption();
		} else {
			$("#customConfirmBox").find("input.confirmFlgChkBox:checked").prop({"checked": false});
			setOption();
		};
	};

	var toggleGenerateThumbnail = function() {
		if ($("input[type=checkbox][name=thumb]:visible").is(':checked')) {
			$("input[type=checkbox][name=thumb]:hidden").prop("checked", true);
		} else {
			$("input[type=checkbox][name=thumb]:hidden").prop("checked", false);
		}
		setOption();
	};

	var toggleClearExif = function() {
		if ($("input[type=checkbox][name=exif]:visible").is(':checked')) {
			$("input[type=checkbox][name=exif]:hidden").prop('checked', true);
		} else {
			$("input[type=checkbox][name=exif]:hidden").prop('checked', false);
		}
		setOption();
	}

	var setNotifyText = function(visible) {
		var num = wu.getValues("operatingMode");
		var className = "";

		if (wu.isDraggable()) {
			switch (num) {
				case 3:
					className = wu.Message.NOTE.CLASS_NAME.DRAGGABLE_MESSAGE_PAID;
					break;
				case 2:
					className = wu.Message.NOTE.CLASS_NAME.DRAGGABLE_MESSAGE_ADULT_PAID;
					break;
				case 1:
					className = wu.Message.NOTE.CLASS_NAME.DRAGGABLE_MESSAGE_FREE;
					break;
				default:
					className = wu.Message.NOTE.CLASS_NAME.DRAGGABLE_MESSAGE_ADULT_FREE;
					break;
			}
		} else {
			switch (num) {
				case 3:
					className = wu.Message.NOTE.CLASS_NAME.SELECTFILES_MESSAGE_PAID;
					break;
				case 2:
					className = wu.Message.NOTE.CLASS_NAME.SELECTFILES_MESSAGE_ADULT_PAID;
					break;
				case 1:
					className = wu.Message.NOTE.CLASS_NAME.SELECTFILES_MESSAGE_FREE;
					break;
				default:
					className = wu.Message.NOTE.CLASS_NAME.SELECTFILES_MESSAGE_ADULT_FREE;
					break;
			}
		};

		if (true === visible) {
			$("#multiFileUploader").find(".notifyDrag").addClass(className).show();
		} else if (!visible) {
			$("#multiFileUploader").find(".notifyDrag").hide().removeClass(className);
		};
	};

	var toggleHeaderRow = function(visible) {
		if (visible) {
			$("#multiFileUploader").find(".headerRow").css("visibility", "visible");
		} else {
			$("#multiFileUploader").find(".headerRow").css("visibility", "hidden");
		};
	};
	var selectBoxFixedIE = function() {
		var elm = [];
		var flg = true;
		var padding = 2;
		var border  = 2;
		var show = function() {
			if (!flg) return;
			$(this).width("auto");
			flg = false;
		};
		var reset = function(e) {
			var self = this;
			if (flg) return;
			var data = e.data;
			var time = setTimeout(function(){
				$(self).width(data.w + "px");
				flg = true;
				clearTimeout(time);
			}, 50);
		};
		var init = function() {
			var selectBox = $("#multiFileUploader").find(".selectAlbumUK");
			for (var i = 0; i < selectBox.length; i++) {
				var w   = $(selectBox[i]).width() + (padding + padding + border + border);
				var obj = {
					obj : selectBox[i],
					w   : w
				};
				elm.push(obj);
				var span = document.createElement("span");
				$(span).css("width", (w) + "px")
					.css("display", "inline-block")
					.css("overflow", "hidden");
				$(selectBox[i]).wrap(span);
				$(selectBox[i])
					.on("mousedown", selectBox[i], show);
				$(selectBox[i])
					.on("change", obj, reset)
					.on("blur", obj, reset)
					.on("select", obj, reset);
			};
		};
		init();
	};

	var doSearch = function(e) {
		if (e.keyCode == 13 && !$("#searchButton:disabled").length) {
			searchFileList();
			return false;
		};
	};

	var onchangeMultipleInput = function() {
		if (!this.files) return;
		for (var i = 0; i < this.files.length; i++) {
			console.log(this.files[i]);
		}
		addUploadCueue(this.files);
		var timer = setTimeout(function(){
			var input = document.createElement("input");
			input.setAttribute("name", "upfile[]");
			input.setAttribute("type", "file");
			input.setAttribute("multiple", true);
			$(input)
				.change(onchangeMultipleInput)
				.css("position", "absolute")
				.css("z-index", "-9999");
			$("#spanButtonPlaceHolder").parent().append(input);
			clearTimeout(timer);
		}, 100);
		$(this).remove();
		$(".style_error").empty();
	};

	var openMultipleFileInput = function() {
		$("#multiFileUploader input[name='upfile[]']").click();
		return false;
	};

	var setupNoSwf = function() {
		var input = document.createElement("input");
		input.setAttribute("name", "upfile[]");
		input.setAttribute("type", "file");
		input.setAttribute("multiple", true);

		$(input)
			.change(onchangeMultipleInput)
			.css("position", "absolute")
			.css("z-index", "-9999");
		$("#spanButtonPlaceHolder").parent().append(input);

		$("#spanButtonPlaceHolder").click(openMultipleFileInput);
	};

	var initFileList = function() {
		if (!page) page = 1;
		searchFileList(page);
		$(document)
			.on("mouseover", ".fileUrl", function(){
				$(this).addClass("mouseover");
			})
			.on("click", ".fileUrl", function(){
				$(this).select();
			})
			.on("mouseout", ".fileUrl", function(){
				$(this).removeClass("mouseover");
			});
	};

	var setSearcValues = function() {
		if ("string" == typeof search.tail) {
			search.tail = (search.tail.match(/^htm/))? "htm" : search.tail;
		}
		$(".search_area").find("input[name=key]").val(search.key);
		$(".search_area").find("select[name=tail]").val(search.tail);
		$(".search_area").find("select[name=sort]").val(search.sort);
	}


	var initialize = function() {
		var $fileUploaderContainer = $("#multiFileUploader");

		config.paramName            = "upfile[]";
		config.placeFolder          = "multiFileUploader";
		config.server               = url;
		config.controlUrl           = controlUrl;
		config.maxsize              = maxsize;
		config.operatingMode        = operatingMode;
		config.callback             = showCueueInfo;
		config.callbackAddedCueue   = showCueueInfo;
		config.callbackErrorAddCueue= errorMessage;
		config.callbackDialogStart  = dialogStart;
		if (!ie) {
			config.swf = false;
		} else {
			config.swf = true;
		}

		/* swf */
		wu         = WebUploader.init(config);
		wu.Message = new Message();
		cc         = new customConfirm();
		album      = new Album(controlUrl);
		cp         = new customPulldown();

		setSearcValues();
		initFileList();

		setNotifyText(true);
		toggleHeaderRow(false);

		if (!ie) {
			setupNoSwf();
		} else {
			if (ie <= 9) {
				$("#multiFileUploader").addClass("lte_ie9");
			}
			$(window).unload(doDestroy);
		}

		if (wu.isDraggable()) {
			$(".dragPanel").on("drop", onDrap)
			.on("dragenter", function(e) {
				e.preventDefault();
				return false;
			})
			.on("dragover", function(e) {
				e.preventDefault();
				return false;
			})
			.on("dragleave", function(e) {
				e.preventDefault();
				$(this).hide();
			})
			.hide();

			$fileUploaderContainer.on("dragenter", function(e) {
				e.preventDefault();
				if (!draggable) return;
				var h = $fileUploaderContainer.find(".FileUploader").height() + "px";
				$("div.dragPanel").height(h).show();
			});
		};
		/* event set */
		/* upload */
		$fileUploaderContainer.find("input.startUploadButton").click(fileManager.event.onclickUploadButton);
		/* cueue cancel */
		$(document)
			.on("click", "#multiFileUploader span.cueueCancelButton", fileManager.event.oncancelCueue)
			.on("focus", "#multiFileUploader input.textboxTitle", fileManager.event.onFocusTitle)
			.on("blur", "#multiFileUploader input.textboxTitle", fileManager.event.onBlurTitle);

		/* アップローダ切替 */
		$(".uploaderToggle").find("span").on("click", fileManager.event.onclickUploaderToggle);
		/* thumb size available */
		$fileUploaderContainer.find("input[name=thumb]").click(function(){
			toggleThumbSize();
		});
		/* 画像をアルバムに追加する */
		$(document).on("click", "#multiFileUploader input[name=album]", function(){
			toggleAlbumCheckBox();
		});
		/* 全てアルバムに追加する */
		$fileUploaderContainer.find("input.albumAllCheck").click(fileManager.event.toggleAlbumAllCheckBox);

		$("select#selectOverwrite").linkselect();
		$("select#selectAlbumUK").linkselect({change: openAddAlbumContainer});

		/* アルバム追加 */
		$("#addAlbumContainer").find("select[name=permission]").on("change", fileManager.event.togglePermissionElm);
		$("#addAlbumContainer").find(".cancel").on("click", function(){
			closeAddAlbumContainer();
		});
		$("#addAlbumContainer").find(".submit").on("click", function(){
			doAddAlbum();
		});

		/* 確認を表示しない (ダイアログ内) */
		$("#customConfirmBox").find("input.confirmFlgChkBox").click(toggleConfirmFlg);
		/* 確認を表示しない2 (共通設定) */
		$fileUploaderContainer.find("input.confirmFlgChkBox").click(toggleConfirmFlg_s);

		/* サムネイル同時に作成 */
		$("input[type=checkbox][name=thumb]").click(toggleGenerateThumbnail);

		/* Exif消去 */
		$("input[name=exif]").click(toggleClearExif);

		/* 検索 */
		$("#searchButton").click(searchFileList);

		$("input[name=key]").keypress(doSearch);
	};
	initialize();

	var initTimer = setTimeout(function(){
		/* 初期状態 */
		if ("0" === uiconfig) {
			// $(".uploaderToggle span:visible").click();
			showHTMLUploadForm();
		} else {
			showAjaxUploadForm();
		};
		if (chkconfig != "") {
			if ("1" === chkconfig) {
				if ($("#customConfirmBox").find("input.confirmFlgChkBox:checked").length) {
					$("#multiFileUploader").find("input.confirmFlgChkBox").prop("checked", false);
				};
			} else {
				$("#multiFileUploader").find("input.confirmFlgChkBox").prop("checked", true);
			};
		};
		if ("undefined" != typeof thumbconfig) {
			if ("1" === thumbconfig) $("input[type=checkbox][name=thumb]").prop("checked", true);
		};
		if ("undefined" != typeof exifconfig) {
			if ("1" === exifconfig) $("input[type=checkbox][name=exif]").prop("checked", true);
		};
		toggleThumbSize();

		if (!isValidateSWF() && wu.isUsingSwf()) {
			noSWF();
		};

		/* キュー一覧の html obj のクローンを作成・元のhtmlは削除 */
		initCueueHtml();
		/* アップローダ表示 */
		$("#multiFileUploader").css("visibility", "");
		/* 独自セレクトボックス */
		if (1 < $("#selectAlbumUK_list li").length) {
			$("#selectAlbumUK").linkselect("val", $("#selectAlbumUK_list li:eq(1)").attr("data-value"));
		} else {
			$("#selectAlbumUK_link").on("mousedown", noAlbumItem);
			isExistAlbum = false;
			$("#multiFileUploader").find(".albumAllCheck").prop("disabled", true);
		};
		clearTimeout(initTimer);
	}, 100);
});
</script>

		<script type="text/javascript">
		$(function() {
			var $target = $('.txt_list');
			$('#toggleTxtListArea').toggle(function() {
				$(this).css('background', 'url(//static.fc2.com/image/icon_main/btn_plus.png) 5px center no-repeat');
				$target.hide();
			},function() {
				$(this).css('background', 'url(//static.fc2.com/image/icon_main/btn_minus.png) 5px center no-repeat');
				$target.show();
			}).click();
		});
		</script>
		<script type="text/javascript">
		function toggleAddAlbumFlg(flg) {
			var form = document.upload;
			for (var i=0; i<=5; i++) {
				form.elements['album['+i+']'][flg].checked = true;
			}
		}
		</script>
