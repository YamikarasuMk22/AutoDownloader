<script type="text/javascript">
$(function(){
	var lang = "ja";
	/* swf button */
	var config = {
		button_image_url: "//static.fc2.com/image/admin/swfupload/ja/upload_select_btn_ja.png?201303",
		button_width: "113",
		button_height: "29",
		button_text: "<span class='theFont'></span>",
		button_text_style: ".theFont { font-size: 12; }",
		button_text_left_padding: 3,
		button_text_top_padding: 4
	}

	var Message = function(key) {
		var max = "2097152";
		var key = key || function() {
			var num = 0;

			if (5000000 < max) num = 3;
			else if (5000000 < max) num = 2;
			else if (2000000 < max) num = 1;
			else if (2000000 < max) num = 0;
			return num;
		};
		var size = { 0: "2MB", 1: "2MB", 2: "5MB", 3: "5MB" }[key()];

		this.CONFIRM = {
			OVER_WRITE_FILE        : "ファイルをアップロードしますか？\n同じ名前のファイルは上書き保存されます。",
			SAVE_AS_FILE           : "ファイルをアップロードしますか？\n同じ名前のファイルは別名で保存します。"
		};
		this.NOTICE = {
			QUEUE_LIMIT_EXCEEDED   : "一度のアップロードで選択できるファイルは20までです。",
			FILE_EXCEEDS_SIZE_LIMIT: "アップロード可能なファイルのサイズは、" + size + "までです。",
			ZERO_BYTE_FILE         : "ファイルサイズが0のファイルはアップロードできません。",
			INVALID_FILETYPE       : "対応していない形式のファイルです。",
			INVALID_FILENAME       : "ファイル名にスラッシュやピリオドを含めることは出来ません。",
			ALBUM_LIMIT_EXCEEDED   : "これ以上アルバムを作成できません。"
		};
		this.RESULT = {
			UPLOAD_COMPLETE        : "アップロード完了",
			UPLOAD_COMPLETE_SUB    : "完了",
			UPLOAD_FAILED          : "アップロードに\n失敗しました。",
